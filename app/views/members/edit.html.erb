<% if !@current_user.nil? and @current_user.logged_in? %>

	<div class="twoColumns-0">
	  <h1>Edit Profile</h1>
	  <% form_for @member, :url => {:controller => 'members', :action => 'update'}, :html => {:multipart => true} do |f| %>
	  <%= f.error_messages %>
	  <%= render "edit_form", :f => f %>
	  <div class="formAction clear">
	    <%= f.submit "Udate" %>
	  </div>
	  <% end %>
	</div>
   		
<% else %>

	<p>You must be logged in to see your account</p>

<% end %>